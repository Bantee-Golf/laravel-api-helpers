{
  "name": "emedia/api",
  "description": "API and custom response helpers for Laravel 6 and 7",
  "type": "package",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@bitbucket.org:elegantmedia/php-helpers.git"
    }
  ],
  "require": {
    "php": "^7.2",
    "laravel/framework": "^6.0|^7.19",
    "emedia/php-helpers": "^2.0",
    "ext-json": "*",
    "symfony/yaml": "^5.1"
  },
  "license": "private",
  "autoload": {
    "psr-4": {
      "EMedia\\Api\\": "src/"
    },
    "files": [
      "IDEAutoComplete.php",
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "IDEAutoComplete.php",
      "app/functions.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "EMedia\\Api\\ApiServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^9.2"
  }
}
